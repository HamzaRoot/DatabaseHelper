        <activity
            android:name=".activites.RoomDatabaseA"
            android:exported="false" />
        <activity
            android:name=".activites.BindingAdapterA"
            android:exported="false" />
        <activity
            android:name=".activites.DataBindingA"
            android:exported="false" />
        <activity
            android:name=".activites.MutableLiveDataA"
            android:exported="false" />
        <activity
            android:name=".activites.QuotesA"
            android:exported="false" />
        <activity
            android:name=".activites.HomeA"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.CalculationA"
            android:exported="false" />
    </application>

</manifest>